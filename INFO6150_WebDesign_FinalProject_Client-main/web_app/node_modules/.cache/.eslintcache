n_FinalProject_Client-main/web_app/src/index.js",[],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/App.js",["167","168","169"],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/reportWebVitals.js",[],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/store.js",[],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/screens/Homescreen.js",["170","171"],[],"import React, { useState, useEffect } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { getAllitems } from \"../actions/itemActions\";\nimport Item from \"../components/Item\";\nimport Loading from \"../components/Loading\";\nimport Error from \"../components/Error\";\nimport Filter from \"../components/Filter\"\n\n//use get all items reducer to display all the items on homescreen\nexport default function Homescreen() {\n  const dispatch = useDispatch();\n\n  const itemsstate = useSelector((state) => state.getAllitemsReducer);\n   //display all the items, error or loading screen depending on the state of webpage  \n  const { items, error, loading } = itemsstate;\n  useEffect(() => {      \n    dispatch(getAllitems());\n  }, []);\n\n  return (\n    <div>\n      <h1><b>CHECK OUT OUR MENU FOR SOME DELICIOUS OPTIONS THAT WILL SATISFY YOUR CRAVINGS.</b></h1>\n      <Filter />\n      <div className=\"row justify-content-center\">\n        \n        {loading ? (\n          <Loading />\n        ) : error ? (\n          <Error />\n        ) : (\n          items.map((item) => {\n            return (\n              <div className=\"col-md-3 p-4\" key={item._id}>\n                <div>\n                  <Item item={item} />\n                </div>\n              </div>\n            );\n          })\n        )}\n        {}\n      </div>\n    </div>\n  );\n}\n",[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/screens/Registerscreen.js",["172","173","174"],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/screens/Cartscreen.js",["175"],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/screens/Loginscreen.js",["176","177"],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/screens/Ordersscreen.js",["178","179","180","181","182","183"],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/screens/Payment.js",["184","185","186","187"],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/screens/Adminscreen.js",["188","189","190"],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/reducers/cartReducer.js",[],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/reducers/itemReducers.js",[],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/reducers/userReducer.js",[],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/reducers/orderReducer.js",[],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/components/Navbar.js",["191"],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/components/Loading.js",[],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/components/Filter.js",["192","193"],[],"import React, { useState, useEffect } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { filterItems } from \"../actions/itemActions\";\n\nexport default function Filter() {\n  //alert boxes to display specific errors\n  const [searchkey, setsearchkey] = useState(\"\");\n  const [category, setcategory] = useState(\"all\");\n  const dispatch = useDispatch();\n  return (\n    <div className=\"container \">\n      <div className=\"row justify-content-center\">\n        <div className=\"col-md-5\">\n          <input\n            onChange={(e) => {\n              setsearchkey(e.target.value);\n            }}\n            value={searchkey}\n            type=\"text\"\n            className=\"form-control\"\n            placeholder=\"Search here\"\n          />\n        </div>\n        <div className=\"col-md-2 mt-2\">\n          <select\n            onChange={(e) => {\n              setcategory(e.target.value);\n            }}\n            value={category}\n            className=\"form-control\"\n          >\n            <option value=\"all\"> ALL</option>\n            <option value=\"coffee\"> Coffee</option>\n            <option value=\"tea\"> Tea</option>\n            <option value=\"sandwiches\"> Sandwiches</option>\n            <option value=\"coldbrew\"> Cold Brew </option>\n          </select>\n        </div>\n        <div className=\"col-md-2\">\n          <button\n            className=\"btn mt-2 bg-dark text-light\"\n            onClick={() => dispatch(filterItems(searchkey, category))}\n          >\n            {\" \"}\n            FILTER\n          </button>\n        </div>\n      </div>\n    </div>\n  );\n}\n","/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/components/Error.js",[],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/components/Item.js",["194","195","196"],[],"import React, { useState } from \"react\";\nimport { Modal } from \"react-bootstrap\";\n\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { addToCart } from \"../actions/cartActions\";\n\n\nexport default function Item({ item }) {\n  const [quantity, setquantity] = useState(1);\n  const [varient, setvarient] = useState(\"small\");\n  const [show, setShow] = useState(false);\n\n  const handleClose = () => setShow(false);\n  const handleShow = () => setShow(true);\n  const dispatch = useDispatch();\n  const userState = useSelector((state) => state.loginUserReducer);\n  const { currentUser } = userState;\n  function addtocart() {\n    \n    if(currentUser == undefined){\n      console.log(\"please Login\");\n      alert(\"Please login to place order\");\n    }\n    dispatch(addToCart(item, quantity, varient));\n  }\n  //cards to display products in grid format\n  return (\n    <div className=\"  p-4 mb-7 bg-transparent\">\n      <div onClick={handleShow}>\n        <h1 className=\"itemname\">{item.name}</h1>\n        <img src={item.image} className=\"img-fluid\" />\n      </div>\n\n      <div className=\"flex-container\">\n        <div className=\"w-100\">\n          <p>Sizes</p>\n          <select\n            className=\"sizes-dropdown\"\n            value={varient}\n            onChange={(e) => {\n              setvarient(e.target.value);\n            }}\n          >\n            {item.varients.map((varient) => {\n              return <option value={varient}>{varient}</option>;\n            })}\n          </select>\n        </div>\n\n        <div className=\"w-100\">\n          <p>Quantity</p>\n          <select\n            value={quantity}\n            onChange={(e) => {\n              setquantity(e.target.value);\n            }}\n          >\n            {[...Array(10).keys()].map((x, i) => {\n              //spread operator\n              return <option value={i + 1}>{i + 1}</option>;\n            })}\n          </select>\n        </div>\n      </div>\n\n      <div className=\"flex-container\">\n        <div className=\"m-3 w-100\">\n          <h1 className=\"price-text\">\n            {\" \"}\n            Price: {item.prices[0][varient] * quantity}$\n          </h1>\n          <button className=\"btn bg-secondary text-light\" onClick={addtocart}>\n            Add To Cart\n          </button>\n      </div>\n      </div>\n      {/* popups */}\n      <div class=\"modal\" tabindex=\"-1\" role=\"dialog\">\n        <div class=\"modal-dialog\" role=\"document\">\n          <div class=\"modal-content\">\n            <div class=\"modal-header\">\n              <h5 class=\"modal-title\">Modal title</h5>\n              <button\n                type=\"button\"\n                class=\"close\"\n                data-dismiss=\"modal\"\n                aria-label=\"Close\"\n              >\n                <span aria-hidden=\"true\">&times;</span>\n              </button>\n            </div>\n            <div class=\"modal-body\">\n              <p>Modal body text goes here.</p>\n            </div>\n            <div class=\"modal-footer\">\n              <button type=\"button\" class=\"btn btn-primary\">\n                Save changes\n              </button>\n              <button\n                type=\"button\"\n                class=\"btn btn-primary\"\n                data-dismiss=\"modal\"\n              >\n                Close\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n      <Modal show={show}>\n        <Modal.Header>\n          <Modal.Title>{item.name}</Modal.Title>\n        </Modal.Header>\n\n        <Modal.Body>\n          <img src={item.image} className=\"img-fluid\"></img>\n          <p>{item.description}</p>\n        </Modal.Body>\n\n        <Modal.Footer>\n          <button className=\"btn bg-primary text-light\" onClick={handleClose}>\n            CLOSE\n          </button>\n        </Modal.Footer>\n      </Modal>\n    </div>\n  );\n}\n","/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/components/Success.js",[],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/components/Checkout.js",[],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/screens/Itemslist.js",["197","198","199"],[],"/Users/rithvik/Desktop/FrontEnd_WebD_Project-3/INFO6150_WebDesign_FinalProject_Client-main/web_app/src/screens/Orderslist.js",["200","201"],[],"import React, { useState, useEffect } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport Loading from \"../components/Loading\";\nimport Error from \"../components/Error\";\nimport { deliverOrder, getAllOrders } from \"../actions/orderAction\";\n\n\n\nconst Orderslist = () => {\n\n    const dispatch = useDispatch()\n    const getordersstate = useSelector(state => state.getAllOrdersReducer)\n    const{loading, error, orders} = getordersstate\n    useEffect(() =>{\n        dispatch(getAllOrders())\n    }, [])\n    return (\n         <div className=\"table-responsive\">\n             {loading && (<Loading/>)}\n                {error &&(<Error error = 'something went wrong'/>)}\n                <table className=\"table table-striped table-borderd\">\n                    <thead >\n                        <tr>\n                            <th> Order ID</th>\n                            <th> Email</th>\n                            <th>User Id</th>\n                            <th>Amount</th>\n                            <th>Date</th>\n                            <th>Status</th>\n                        </tr>\n                    </thead>\n                    <tbody>\n                        {orders && orders.map(order => {\n                            return <tr>\n                                <td>{order._id}</td>\n                                <td>{order.email}</td>\n                                <td>{order.userid}</td>\n                                <td>{order.orderAmount}</td>\n                                <td>{order.createdAt.substring(0,10)}</td>\n                        